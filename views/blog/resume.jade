style(type="text/css")
    #console {
        padding: 15px;
        height: 500px;
        width: 700px;
        margin: auto;
        border-radius: 5px;
        overflow: auto;
        font-size: 15px;
    }
#console
script(type="text/javascript")
    var shell = $('#console').shotgunConsole();
    var $line = $('<div>').appendTo(shell.$display);
    $line.coolType('Loading interactive résumé program. Please wait...', {
        delayAfterType: 0,
        onComplete: function () {
            shell.execute('welcome');
        }
    });